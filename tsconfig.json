{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/ui/card.jsx", "src/app/control-pagos/page.jsx", "src/app/components/vistas-admin/tabla-pagos.jsx", "src/app/control-pagos/page.jsx", "src/app/components/vistas-admin/tabla-pagos.jsx", "src/app/control-pagos/page.jsx", "src/app/components/vistas-admin/tabla-pagos.jsx", "src/app/components/filtro-cedula.jsx", "src/app/control-pagos/page.jsx", "src/app/control-notas/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
